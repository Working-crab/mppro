{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Python: FastAPI",
      "type": "python",
      "request": "launch",
      "module": "uvicorn",
      "args": [
        "ui_backend.main:app",
        "--reload"
      ],
      "python": "${workspaceFolder}/venv/bin/python3",
      "env": {
        "PYTHONPATH": "src/"
      },
      "jinja": true,
      "justMyCode": true
    },
    {
      "name": "Python: Kafka",
      "type": "python",
      "request": "launch",
      "program": "${workspaceFolder}/kafka_test.py",
      "python": "${workspaceFolder}/venv/bin/python3",
      "jinja": true,
      "justMyCode": true
    }
  ]
}